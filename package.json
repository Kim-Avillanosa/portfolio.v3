{
  "name": "nextlaunch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "npm run lint",
    "compile:tailwind": "npx tailwindcss -i ./src/styles/globals.css -o ./public/output.css",
    "build:dev": "npm run compile:tailwind && env-cmd -f ./variables/.env.development next build",
    "build:uat": "npm run compile:tailwind && env-cmd -f ./variables/.env.uat next build",
    "build:prod": "npm run compile:tailwind && env-cmd -f ./variables/.env.production next build",
    "dev": "env-cmd -f ./variables/.env.development next dev",
    "uat": "env-cmd -f ./variables/.env.uat next dev",
    "prod": "env-cmd -f ./variables/.env.production next start"
  },
  "dependencies": {
    "@nextui-org/react": "^2.1.13",
    "@nextui-org/system": "^2.0.10",
    "@nextui-org/theme": "^2.1.9",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "axios": "^1.5.0",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.6",
    "env-cmd": "^10.1.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "flowbite": "^1.8.1",
    "flowbite-react": "^0.6.0",
    "formik": "^2.4.4",
    "framer-motion": "^10.16.4",
    "next": "^13.5.4",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-grid-system": "^8.1.9",
    "react-type-animation": "^3.1.0",
    "rxjs": "^7.8.1",
    "swr": "^2.2.4",
    "typescript": "5.2.2",
    "yup": "^1.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3"
  }
}
